    <% for (var i=0, files=data.files, l=files.length, file=files[0]; i<l; file=files[++i]) { %>
    <tr class="template-download file-row">
        <td class="image">
            <% if (file.thumbnail_url) { %>
            <img src="<%=file.thumbnail_url%>">
            <% } %>
        </td>
        <td class="name"><a href="<%=file.url%>" title="<%=file.name%>" rel="gallery"><%= file.name %></a></td>
        <td class="price-holder"><strong class="price"><%=data.formatFileSize(file.size)%></strong></td>
        <% if (file.error) { %>
        <td class="error">
            <div style="padding: 0px 0.7em;" class="ui-state-error ui-corner-all">
                <p><span style="margin-right: 0.3em; float: left;" class="ui-icon ui-icon-alert"></span>
                    <strong>Error:</strong> <%= (fileUploadErrors[file.error] || file.error) %></p>
            </div>
        </td>
        <% } else { %>
        <td><strong>Successfully uploaded</strong></td>
        <% } %>
        <td></td>
<!--
        <td class="delete">
            <button class="btn danger" data-type="{%=file.delete_type%}" data-url="{%=file.delete_url%}">Delete</button>
            <input type="checkbox" name="delete" value="1">
        </td>
-->
    </tr>

    <% } %>
    <% $('tr.file-row:odd').addClass('odd'); %>